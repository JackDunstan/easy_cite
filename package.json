{
  "devDependencies": {
    "browser-sync": "^3.0.3",
    "browsersync": "^0.0.1-security",
    "concurrently": "^9.1.0",
    "prettier": "^3.4.2",
    "sass": "^1.82.0"
  },
  "name": "easy_cite",
  "version": "1.0.0",
  "description": "Easy Cite referencing tool by [RMIT University Library](https://www.rmit.edu.au/library) is licensed under [Creative Commons CC BY-NC-SA 3.0](https://creativecommons.org/licenses/by-nc-sa/3.0/).",
  "main": "index.js",
  "dependencies": {
    "braces": "^3.0.3",
    "chokidar": "^4.0.1",
    "detect-libc": "^1.0.3",
    "fill-range": "^7.1.1",
    "immutable": "^5.0.3",
    "is-extglob": "^2.1.1",
    "is-glob": "^4.0.3",
    "is-number": "^7.0.0",
    "micromatch": "^4.0.8",
    "node-addon-api": "^7.1.1",
    "picomatch": "^2.3.1",
    "readdirp": "^4.0.2",
    "source-map-js": "^1.2.1",
    "to-regex-range": "^5.0.1"
  },
  "scripts": {
    "sass": "sass --watch sass/main-new.scss:css/main-new.css --source-map",
    "serve": "browser-sync start --proxy 'http://easy_cite.test/' --files 'css/*.css, *.html, *.php'",
    "start": "concurrently \"npm run sass\" \"npm run serve\"",
    "format": "prettier --write '**/*.{js,css,scss}'"
  },
  "keywords": [],

{
  "devDependencies": {
    "browser-sync": "^3.0.3",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.6",
    "cypress": "^13.16.1",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.4.2",
    "sass": "^1.82.0",
    "terser": "^5.37.0"
  },
  "name": "easy_cite",
  "version": "1.0.0",
  "description": "Easy Cite referencing tool by [RMIT University Library](https://www.rmit.edu.au/library) is licensed under [Creative Commons CC BY-NC-SA 3.0](https://creativecommons.org/licenses/by-nc-sa/3.0/).",
  "config": {
    "baseUrl": "easy_cite.test"
  },
  "main": "index.js",
  "dependencies": {
    "braces": "^3.0.3",
    "chokidar": "^4.0.1",
    "detect-libc": "^1.0.3",
    "fill-range": "^7.1.1",
    "immutable": "^5.0.3",
    "is-extglob": "^2.1.1",
    "is-glob": "^4.0.3",
    "is-number": "^7.0.0",
    "micromatch": "^4.0.8",
    "node-addon-api": "^7.1.1",
    "picomatch": "^2.3.1",
    "readdirp": "^4.0.2",
    "source-map-js": "^1.2.1",
    "to-regex-range": "^5.0.1"
  },
  "scripts": {
    "build:css": "npm run scss:compile && npm run css:minify",
    "build:js": "npm run js:minify",
    "build": "concurrently 'npm run build:css' 'npm run build:js'",
    "watch:scss": "chokidar 'assets/sass/**/*.scss' -c 'npm run build:css' --ignore 'assets/**/*.min.css'",
    "watch:js": "chokidar 'assets/js/**/*.js' -c 'npm run build:js' --ignore 'assets/js/**/*.min.js'",
    "serve": "cross-env browser-sync start --proxy http://$npm_package_config_baseUrl --host $npm_package_config_baseUrl --files 'assets/css/*.css, assets/**/*.js, **/*.php, *.html'",
    "start": "concurrently 'npm run watch:scss' 'npm run watch:js' 'npm run serve'",
    "scss:compile": "sass assets/sass/styles.scss assets/css/styles.css --source-map",
    "css:minify": "postcss assets/css/styles.css --use cssnano -o assets/css/styles.min.css --map",
    "js:minify": "terser assets/js/*.js -o assets/js/scripts.min.js",
    "format": "prettier --write '**/*.{js,css,scss}'",
    "cypress:run": "cross-env CYPRESS_baseUrl=http://$npm_package_config_baseUrl cypress run",
    "cypress:open": "cross-env CYPRESS_baseUrl=http://$npm_package_config_baseUrl cypress open"
  },
  "keywords": [],
  "author": "",
  "license": "CC-BY-NC-SA-3.0",
  "engines": {
    "php": ">=7.4",
    "node": ">=22.12.0",
    "npm": ">=10.9.0"
  }
}
